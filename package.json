{
	"name": "@affinity-lab/graphane-frontend",
	"version": "0.0.1",
	"author": "Affinity Lab",
	"main": "dist/index",
	"scripts": {
		"rebuild": "find . -type d -depth 1 ! -name \"@*\" ! -name \".*\" ! -name \"node_modules\" -exec rm -r {} \\; & tsc",
		"idev": "cp package.json temp.json && cp package.dev.json package.json && npm install && rm -rf package.json && cp temp.json package.json && rm -rf temp.json"
	},
	"files": [
		"./**/*"
	],
	"dependencies": {
		"micromatch": "^4.0.5",
		"@sveltejs/kit": "^1.25.0",
		"jsonwebtoken": "^9.0.2"
	},
	"devDependencies": {
		"typescript": "^5.2.2",
		"@types/node": "^20.6.0",
		"@types/micromatch": "^4.0.2",
		"@types/jsonwebtoken": "^9.0.2"
	}
}
